<includes>

	<include name="background_Home">
		<param name="DiffuseTexture"/>
		<param name="GridTexture" default="overlays/90.png"/>
		<param name="DiffuseColor"/>
		<param name="Visible" default="!Window.IsVisible(fullscreenvideo) + ![Player.HasVideo + Skin.HasSetting(global.backgroundvideo)]"/>
		<definition>

			<!-- No Background -->
			<control type="image">
				<include>screen_Dimensions</include>
				<texture colordiffuse="$VAR[color:Main]">colorize/white.png</texture>
				<aspectratio>stretch</aspectratio>
				<visible>String.IsEqual(Skin.String(home.background),none) | Window.IsActive(home) + String.IsEqual(Skin.String(homemenu.layout),compact)</visible>
			</control>

			<control type="group">
				<include>transition_Background</include>

				<!-- Background Slideshow -->
				<control type="multiimage">
					<timeperimage>30000</timeperimage>
					<fadetime>300</fadetime>
					<pauseatend>10000</pauseatend>
					<randomize>true</randomize>
					<include>screen_Dimensions</include>
					<include>Burns</include>
					<aspectratio>scale</aspectratio>
					<imagepath background="true">$VAR[background:Home]</imagepath>

					<visible>!String.IsEqual(Skin.String(home.background),none) + !String.IsEqual(Skin.String(homemenu.layout),compact) + ![Player.HasAudio + Skin.HasSetting(home.nowplaying)]</visible>
				</control>

				<!-- Background video -->
				<include>background_Video</include>

				<!-- Now playing -->
				<control type="image">
					<include>screen_Dimensions</include>
					<texture>$VAR[player:Art]</texture>
					<fadetime>fadetime_Thumb</fadetime>
					<include>fadeinout</include>
					<aspectratio align="center" aligny="center">scale</aspectratio>
					<visible>Player.HasAudio + Skin.HasSetting(home.nowplaying)</visible>
				</control>

				<!-- Overlay -->
				<control type="group">
					<visible>String.IsEqual(Skin.String(homemenu.layout),vertical)</visible>
					<control type="image">
						<include>screen_Dimensions</include>
						<texture background="true" diffuse="$PARAM[DiffuseTexture]">overlays/background.png</texture>
						<colordiffuse>$VAR[color:Main]</colordiffuse> 
						<fadetime>fadetime_Thumb</fadetime>
					</control>
					<control type="image">
						<colordiffuse>$VAR[color:Accent]</colordiffuse>
						<include>screen_Dimensions</include>
						<texture background="true" diffuse="$PARAM[DiffuseTexture]" colordiffuse="$PARAM[DiffuseColor]">$PARAM[GridTexture]</texture>
						<include>fadeinout</include>
						<visible>!ControlGroup(9100).HasFocus()</visible>
					</control>
				</control>

				<!-- Logos -->
				<control type="image">
					<right>64</right>
					<bottom>90</bottom>
					<width>330</width>
					<height>128</height>
					<texture>$VAR[home:SlideShowLogo]</texture>
					<aspectratio>keep</aspectratio>
					<fadetime>300</fadetime>
					<visible>String.IsEqual(Skin.String(home.background),fanart) | String.IsEqual(Skin.String(home.background),videofanart)</visible>
					<visible>![Player.HasVideo + Skin.HasSetting(global.backgroundvideo)] + ![Player.HasAudio + Skin.HasSetting(home.nowplaying)]</visible>
					<visible>!Window.IsActive(movieinformation) + Skin.HasSetting(fanart.sourcelogo) + [String.IsEqual(Skin.String(fanart.source),global) | String.IsEqual(Skin.String(fanart.source),video)]</visible>
				</control>
			</control>
		</definition>
	</include>

	<include name="background_Global">
		<param name="DiffuseTexture"/>
		<param name="GridTexture" default="overlays/90.png"/>
		<param name="DiffuseColor"/>
		<param name="RestrictFX" default="false"/>
		<param name="Visible" default="!Window.IsVisible(fullscreenvideo) + ![Player.HasVideo + Skin.HasSetting(global.backgroundvideo)]"/>
		<definition>

			<!-- Colorbox image -->

			<control type="multiimage" id="7975">
				<left>-2160</left>
				<width>1</width>
				<height>1</height>
				<timeperimage>10000</timeperimage>
				<randomize>true</randomize>
				<fadetime>600</fadetime>
				<imagepath background="true">$VAR[background:Global]</imagepath>
			</control>

			<control type="group">
				<include>transition_Background</include>
				<!-- No Background -->
				<control type="image">
					<include>screen_Dimensions</include>
					<texture colordiffuse="$VAR[color:Main]">colorize/white.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>String.IsEqual(Skin.String(global.background),none) | Window.IsActive(home) + String.IsEqual(Skin.String(homemenu.layout),compact)</visible>
				</control>
				
				<!-- Custom slideshow -->
				<control type="multiimage">
					<timeperimage>30000</timeperimage>
					<fadetime>600</fadetime>
					<pauseatend>10000</pauseatend>
					<randomize>true</randomize>
					<include>screen_Dimensions</include>
					<include>Burns</include>
					<aspectratio>scale</aspectratio>
					<imagepath background="true">$VAR[background:Global]</imagepath>
					<animation type="Conditional" condition="ControlGroup(9100).HasFocus()">
						<effect type="Fade" time="250" start="100" end="0" tween="cubic" easing="InOut"/>
					</animation>
					<visible>!String.IsEqual(Skin.String(global.background),none)</visible>
				</control>

				<!-- Fanart background -->
				<control type="image">
					<include>screen_Dimensions</include>
					<include>Burns</include>
					<fadetime>600</fadetime>
					<aspectratio>scale</aspectratio>
					<include>fadeinout</include>
					<texture background="true">$VAR[background:Path]</texture>
					<visible>![Container.Scrolling + Skin.HasSetting(fanart.scrolling)]</visible>
					<visible>![Window.IsActive(Home) + !String.IsEqual(Skin.String(homemenu.layout),vertical)]</visible>
					<visible>String.IsEqual(Skin.String(global.background),fanart) | String.IsEqual(Skin.String(global.background),videofanart)</visible>
				</control>
				
				<!-- Background video -->
				<include>background_Video</include>

				<!-- Overlay -->
				<control type="group">
					<control type="image">
						<include>screen_Dimensions</include>
						<texture background="true" diffuse="$PARAM[DiffuseTexture]">overlays/background.png</texture>
						<colordiffuse>$VAR[color:Main]</colordiffuse> 
						<fadetime>fadetime_Thumb</fadetime>
					</control>
					<control type="image">
						<colordiffuse>$VAR[color:Accent]</colordiffuse>
						<include>screen_Dimensions</include>
						<texture background="true" diffuse="$PARAM[DiffuseTexture]" colordiffuse="$PARAM[DiffuseColor]">$PARAM[GridTexture]</texture>
					</control>
				</control>
			</control>
		</definition>
	</include>

	<include name="background_Video">
		<control type="group">
			<visible>Skin.HasSetting(global.backgroundvideo) + Player.HasVideo + ![String.IsEqual(Skin.String(homemenu.layout),compact) + Window.IsActive(home)]</visible>
			<control type="image">
				<aspectratio>stretch</aspectratio>
				<include>screen_Dimensions</include>
				<texture colordiffuse="ff000000">colorize/white.png</texture>
			</control>
			<control type="videowindow">
				<include>screen_Dimensions</include>
			</control>
		</control>
	</include>

	<include name="background_Player">
		<control type="group">
			<animation type="conditional" condition="Skin.HasSetting(burns.enable)" loop="true">
				<effect type="Fade" start="0" end="100" time="2000"/>
				<effect type="Zoom" center="screen_Center" start="130" end="100" time="15000"/>
				<effect type="Fade" start="100" end="0" time="1000" delay="14000"/>
			</animation>
			<control type="visualisation" id="2">
				<include>screen_Dimensions</include>
				<visible>String.IsEqual(Skin.String(viz.mode),viz) | String.IsEqual(Skin.String(viz.mode),blend)</visible>
			</control>
			<control type="multiimage">
				<include>screen_Dimensions</include>
				<aspectratio align="center">scale</aspectratio>
				<randomize>true</randomize>
				<loop>true</loop>
				<timeperimage>8000</timeperimage>
				<fadetime>fadetime_Slideshow</fadetime>
				<imagepath background="true">$INFO[Window(Visualisation).Property(ArtistSlideshow)]</imagepath>
				<animation effect="fade" end="60" time="250" condition="String.IsEqual(Skin.String(viz.mode),blend)">Conditional</animation>
				<visible>!String.IsEqual(Skin.String(viz.mode),viz) + [System.HasAddon(script.artistslideshow) + !String.IsEmpty(Window(Visualisation).Property(ArtistSlideshow)) + !String.IsEmpty(Window.Property(ArtistSlideshow.ArtworkReady))]</visible>
			</control>
			<control type="image">
				<include>screen_Dimensions</include>
				<texture>$VAR[player:Art]</texture>
				<fadetime>fadetime_Thumb</fadetime>
				<aspectratio align="center" aligny="center">scale</aspectratio>
				<animation effect="fade" end="60" time="250" condition="String.IsEqual(Skin.String(viz.mode),blend)">Conditional</animation>
				<visible>!String.IsEqual(Skin.String(viz.mode),viz) + [String.IsEmpty(Window(Visualisation).Property(ArtistSlideshow)) | String.IsEmpty(Window.Property(ArtistSlideshow.ArtworkReady))]</visible>
			</control>
		</control>
	</include>

	<include name="global_WindowChrome">
		<param name="Top" default="0"/>
		<param name="TextWidth" default="1600"/>
		<param name="Width" default="screen_Width"/>
		<param name="TextureWidth" default="1664"/>
		<param name="Height" default="108"/>
		<param name="Font" default="Black-48"/>
		<param name="Symbol" default="$VAR[window:Symbol]"/>
		<param name="ShowSymbol" default="true"/>
		<param name="SymbolFont" default="Symbols-32"/>
		<param name="ItemCount" default="$INFO[Container.NumItems]"/>
		<param name="TimeLeft" default="1792"/>
		<param name="Title" default="$INFO[Control.GetLabel(1)]"/>
		<param name="ShowItemCount" default="True" />
		<definition>
			<control type="group">
				<height>108</height>
				<width>screen_Width</width>
				<include>transition_Chrome</include>
				<control type="image">
					<width>128</width>
					<height>$PARAM[Height]</height>
					<texture colordiffuse="ff000000">colorize/30.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>!Window.IsActive(Home)</visible>
				</control>
				<control type="label">
					<width>128</width>
					<height>$PARAM[Height]</height>
					<label>$PARAM[ItemCount]</label>
					<font>Thin-48</font>
					<align>center</align>
					<textcolor>$VAR[color:Accent]</textcolor>
					<visible>$PARAM[ShowItemCount]</visible>
					<visible>$EXP[!Window.IsInfo] + ![Window.IsVisible(volumebar) | Player.Muted] + !Window.IsVisible(busydialog)</visible>
				</control>
				<control type="label">
					<width>128</width>
					<height>$PARAM[Height]</height>
					<label>$PARAM[Symbol]</label>
					<font>$PARAM[SymbolFont]</font>
					<align>center</align>
					<textcolor>$VAR[color:Accent]</textcolor>
					<visible>!Window.IsVisible(busydialog)</visible>
					<visible>$PARAM[ShowSymbol]</visible>
				</control>
				<include content="global_Busy">
					<param name="Left">32</param>
					<param name="Top">22</param>
					<param name="Size">64</param>
					<param name="Visible">Window.IsVisible(busydialog)</param>
				</include>
				<control type="image">
					<left>128</left>
					<width>$PARAM[TextureWidth]</width>
					<height>$PARAM[Height]</height>
					<texture colordiffuse="ff000000">colorize/20.png</texture>
					<aspectratio>stretch</aspectratio>
					<visible>!Window.IsActive(Home)</visible>
				</control>

				<!-- Volume slider -->
				<control type="progress" id="29999">
					<left>128</left>
					<width>$PARAM[TextureWidth]</width>
					<height>$PARAM[Height]</height>
					<info>Player.Volume</info>
					<texturebg colordiffuse="$VAR[color:Accent]">colorize/0.png</texturebg>
					<lefttexture></lefttexture>
					<midtexture colordiffuse="$VAR[color:Accent]">colorize/25.png</midtexture>
					<righttexture></righttexture>
					<visible>Window.IsActive(volumebar) + IntegerGreaterThan(Player.Volume,0)</visible>
				</control>

				<!-- Player progress -->
				<control type="progress" id="12">
					<left>128</left>
					<width>1664</width>
					<height>108</height>
					<info>Player.Progress</info>
					<texturebg colordiffuse="$VAR[color:Accent]">progress/player/0.png</texturebg>
					<lefttexture></lefttexture>
					<midtexture colordiffuse="$VAR[color:Accent]">progress/player/50.png</midtexture>
					<righttexture></righttexture>
					<visible>Window.IsVisible(seekbar) + !Window.IsActive(volumebar)</visible>
					<include>transition_Chrome</include>
				</control>

				<!-- Player progress -->
				<control type="progress" id="20">
					<left>128</left>
					<width>1664</width>
					<height>108</height>
					<info>Player.Progressbar</info>
					<texturebg colordiffuse="$VAR[color:Accent]">progress/player/0.png</texturebg>
					<lefttexture></lefttexture>
					<midtexture colordiffuse="$VAR[color:Accent]">progress/player/50.png</midtexture>
					<righttexture></righttexture>
					<include>transition_Chrome</include>
					<visible>Window.IsVisible(progressdialog)</visible>
				</control>

				<!-- Window title -->
				<control type="label" id="1">
					<visible>false</visible>
				</control>
				<control type="grouplist">
					<left>160</left>
					<width>1600</width>
					<height>$PARAM[Height]</height>
					<orientation>vertical</orientation>
					<control type="label">
						<width>1280</width>
						<height>$PARAM[Height]</height>
						<aligny>center</aligny>
						<font>$PARAM[Font]</font>
						<textcolor>$VAR[color:Accent]</textcolor>
						<label>$PARAM[Title]</label>
						<visible>$EXP[!Window.HasOverlay]</visible>
					</control>
					<control type="label">
						<width>1600</width>
						<height>$PARAM[Height]</height>
						<aligny>center</aligny>
						<font>$PARAM[Font]</font>
						<textcolor>$VAR[color:Accent]</textcolor>
						<label>$PARAM[Title]</label>
						<visible>$EXP[!Window.HasOverlay]</visible>
					</control>
					<control type="label">
						<width>832</width>
						<height>$PARAM[Height]</height>
						<aligny>center</aligny>
						<font>$PARAM[Font]</font>
						<textcolor>$VAR[color:Accent]</textcolor>
						<label>$PARAM[Title]</label>
						<visible>$EXP[Window.HasOverlay]</visible>
					</control>
				</control>

				<control type="grouplist">
					<right>128</right>
					<orientation>horizontal</orientation>
					<align>right</align>
					<include>slide_Clock</include>
					<control type="button">
						<width>auto</width>
						<height>$PARAM[Height]</height>
						<label>$INFO[System.Date]</label>
						<font>Regular-22-L</font>
						<align>right</align>
						<aligny>center</aligny>
						<textcolor>$VAR[color:Home.3]</textcolor>
						<texturenofocus></texturenofocus>
						<visible>!String.IsEqual(Skin.String(homemenu.layout),compact)</visible>
						<visible>$EXP[!Window.HasOverlay]</visible>
						<visible>!Window.IsActive(extendedprogressdialog) + !Window.IsActive(notification) + !Player.HasMedia + [Window.IsActive(home) + [String.IsEqual(Skin.String(date.enabled),global) | String.IsEqual(Skin.String(date.enabled),home)]] | [!Window.IsActive(home) + [String.IsEqual(Skin.String(date.enabled),global) | String.IsEqual(Skin.String(date.enabled),nohome)]]</visible>
						<animation effect="slide" end="128" time="0" condition="!Skin.HasSetting(home.clock) + Window.IsActive(home)">Conditional</animation>
					</control>
				</control>
				
				<!-- Clock -->
				<control type="group">
					<top>0</top>
					<left>$PARAM[TimeLeft]</left>
					<width>128</width>
					<height>$PARAM[Height]</height>
					<include>slide_Clock</include>
					<visible>![Window.IsActive(home) + !Skin.HasSetting(home.clock) + !Window.IsActive(movieinformation)] | Window.IsActive(home) + Window.IsVisible(volumebar) + !Player.Muted</visible>
					<control type="image">
						<width>100%</width>
						<height>$PARAM[Height]</height>
						<texture colordiffuse="ff000000">colorize/30.png</texture>
						<aspectratio>stretch</aspectratio>
					</control>
					<control type="grouplist">
						<width>128</width>
						<height>$PARAM[Height]</height>
						<itemgap>0</itemgap>
						<align>center</align>
						<orientation>horizontal</orientation>
						<control type="label">
							<width>55.32</width>
							<height>$PARAM[Height]</height>
							<label>$INFO[System.Time(hh)]</label>
							<font>Black-28</font>
							<align>right</align>
							<aligny>center</aligny>
							<textcolor>$VAR[color:Accent]</textcolor>
						</control>
						<control type="list">
							<width>17.33</width>
							<height>$PARAM[Height]</height>
							<scrolltime>0</scrolltime>
							<autoscroll time="750">true</autoscroll>
							<visible>true</visible>
							<orientation>vertical</orientation>
							<itemlayout height="$PARAM[Height]">
								<control type="label">
									<width>17.33</width>
									<height>$PARAM[Height]</height>
									<align>center</align>
									<aligny>center</aligny>
									<font>Black-22</font>
									<textcolor>$VAR[color:Accent]</textcolor>
									<label>$INFO[ListItem.Label]</label>
								</control>
							</itemlayout>
							<focusedlayout height="$PARAM[Height]">
								<control type="label">
									<width>17.33</width>
									<height>$PARAM[Height]</height>
									<align>center</align>
									<aligny>center</aligny>
									<font>Black-22</font>
									<textcolor>$VAR[color:Accent]</textcolor>
									<label>$INFO[ListItem.Label]</label>
								</control>
							</focusedlayout>
							<content>
								<item>
									<label>&#32;</label>
									<onclick>noop</onclick>
								</item>
								<item>
									<label>&#58;</label>
									<onclick>noop</onclick>
								</item>
							</content>
						</control>
						<control type="label">
							<width>55.32</width>
							<height>$PARAM[Height]</height>
							<label>$INFO[System.Time(mm)]</label>
							<font>Black-28</font>
							<align>left</align>
							<aligny>center</aligny>
							<textcolor>$VAR[color:Accent]</textcolor>
						</control>
					</control>
				</control>
			</control>
		</definition>
	</include>

	<include name="global_Busy">
		<param name="Left"/>
		<param name="CenterLeft"/>
		<param name="Top"/>
		<param name="Size" default="64"/>
		<param name="Visible"/>
		<definition>
			<control type="multiimage">
				<centerleft>$PARAM[CenterLeft]</centerleft>
				<left>$PARAM[Left]</left>
				<top>$PARAM[Top]</top>
				<width>$PARAM[Size]</width>
				<height>$PARAM[Size]</height>
				<aspectratio>keep</aspectratio>
				<imagepath>busy</imagepath>
				<timeperimage>150</timeperimage>
				<fadetime>250</fadetime>
				<pauseatend>500</pauseatend>
				<randomize>false</randomize>
				<colordiffuse>$VAR[color:Accent]</colordiffuse>
				<include>fadeinout</include>
				<visible>$PARAM[Visible]</visible>
			</control>
		</definition>
	</include>

	<include name="global_Notification">
		<param name="Left" default="1164"/>
		<param name="Top" default="0"/>
		<param name="InfoTop" default="16"/>
		<param name="Height" default="108"/>
		<param name="Width" default="756"/>
		<param name="TextWidth" default="600"/>
		<param name="Label"/>
		<param name="Label2"/>
		<param name="ID1"/>
		<param name="ID2"/>
		<param name="Label1"/>
		<param name="Label2"/>
		<param name="ProgressID"/>
		<param name="ProgressInfo"/>
		<param name="Symbol" default="&#57483;"/>
		<param name="ShowProgress" default="true"/>
		<param name="ShowFrame" default="true"/>
		<param name="Visible"/>
		<param name="DiffuseLevel" default="50"/>
		<param name="TextColor" default="$VAR[color:Main]"/>
		<definition>
			<control type="group">
				<top>$PARAM[Top]</top>
				<left>$PARAM[Left]</left>
				<width>$PARAM[Width]</width>
				<height>$PARAM[Height]</height>
				<include>transition_Chrome</include>
				<visible>!Window.IsActive(startup)</visible>

				<!-- Use the current control ID as for some reason the animation is not executed when based on the widgets' control group ID -->
				<animation effect="slide" end="-10,10" time="150" tween="Quadratic" easing="In" delay="250" condition="Window.IsActive(Home) + String.Contains(System.CurrentControlID,100) + !Skin.HasSetting(home.clock)">Conditional</animation>
				<animation effect="slide" end="118,10" time="150" tween="Quadratic" easing="In" delay="250" condition="Window.IsActive(Home) + String.Contains(System.CurrentControlID,100) + Skin.HasSetting(home.clock)">Conditional</animation>

				<control type="image">
					<width>$PARAM[Width]</width>
					<height>$PARAM[Height]</height>
					<texture colordiffuse="$VAR[color:Accent]">colorize/$PARAM[DiffuseLevel].png</texture>
					<visible>$PARAM[ShowFrame]</visible>
				</control>

				<control type="group">
					<include>transition_Chrome</include>
					<control type="group">
						<control type="progress" id="$PARAM[ProgressID]">
							<width>100%</width>
							<height>$PARAM[Height]</height>
							<info>$PARAM[ProgressInfo]</info>Player.Progress
							<lefttexture></lefttexture>
							<midtexture colordiffuse="$VAR[color:Accent]">colorize/100.png</midtexture>
							<righttexture></righttexture>
							<visible>[Player.HasMedia + Window.IsVisible(1105)] | Window.IsActive(extendedprogressdialog)</visible>
							<visible>$PARAM[ShowProgress]</visible>
						</control>
						<control type="group">
							<top>$PARAM[InfoTop]</top>
							<left>108</left>
							<control type="label" id="$PARAM[ID1]">
								<width>$PARAM[TextWidth]</width>
								<height>54</height>
								<font>Black-22</font>
								<label>$PARAM[Label1]</label>
								<textcolor>$PARAM[TextColor]</textcolor>
							</control>
							<control type="label" id="$PARAM[ID2]">
								<top>38</top>
								<width>$PARAM[TextWidth]</width>
								<height>54</height>
								<aligny>top</aligny>
								<font>Regular-20</font>
								<label>$PARAM[Label2]</label>
								<textcolor>$PARAM[TextColor]</textcolor>
							</control>
						</control>
					</control>

					<!-- Symbol -->
					<control type="label">
						<width>108</width>
						<height>$PARAM[Height]</height>
						<font>Symbols-38</font>
						<label>$PARAM[Symbol]</label>
						<align>center</align>
						<aligny>center</aligny>
